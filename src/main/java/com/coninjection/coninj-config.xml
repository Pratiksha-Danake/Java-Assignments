<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
	xmlns:c="http://www.springframework.org/schema/c">
	<bean class="com.coninjection.Certificate" name="certificate" c:name="Java Certification" />
	<bean name="person" class="com.coninjection.Person">
		<constructor-arg value="Pratiksha" />
		<constructor-arg value="13" type="int" />
		<constructor-arg ref="certificate" />
		<constructor-arg>
			<list>
				<value>C</value>
				<value>Java</value>
			</list>
		</constructor-arg>
	</bean>
</beans>